                            // ========================================
                            if (objViagem.dataFinalRecorrencia)
                            {
                                const txtFinalRecorrencia = document.getElementById("txtFinalRecorrencia");
                                if (txtFinalRecorrencia && txtFinalRecorrencia.ej2_instances && txtFinalRecorrencia.ej2_instances[0])
                                {
