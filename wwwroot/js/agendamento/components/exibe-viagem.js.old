                    setTimeout(() =>
                    {
                        console.log("√∞≈∏‚Äú‚Ä¶ [FINAL] Preenchendo lstDias...");

                        const lstDias = document.getElementById("lstDias");
                        if (lstDias && lstDias.ej2_instances && lstDias.ej2_instances[0])
                        {
