                                }
                            }

                            // ========================================
                            // Desabilitar lstPeriodos (UMA VEZ)
                            // ========================================
                            const lstPeriodos = document.getElementById("lstPeriodos");
                            if (lstPeriodos && lstPeriodos.ej2_instances && lstPeriodos.ej2_instances[0])
