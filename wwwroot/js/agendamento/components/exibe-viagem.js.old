                    {
                        const horas = String(dataHora.getHours()).padStart(2, '0');
                        const minutos = String(dataHora.getMinutes()).padStart(2, '0');
                        horaFinalParaExibir = `${horas}:${minutos}`;
                    }
                } catch (e)
