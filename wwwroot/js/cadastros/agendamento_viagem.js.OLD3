async function excluirAgendamento(viagemId)
{
    try
    {
        var objAgendamento = JSON.stringify({
            ViagemId: viagemId
        });
        $.ajax({
            type: "post",
            url: "/api/Agenda/ApagaAgendamento",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            data: objAgendamento,
            success: function (data)
            {
                try
                {
                    if (data.success)
                    {
                        toastr.success(data.message);
                        $("#modalViagens").modal("hide"); // usa a API do Bootstrap
                        $(document.body).removeClass("modal-open");
                        $(".modal-backdrop").remove(); // REMOVE o elemento overlay
                        $(document.body).css("overflow", ""); // deixa o Bootstrap controlar
                        //    location.reload();
                        calendar.refetchEvents(); // Atualiza sÃ³ os eventos!
                    } else
                    {
                        toastr.error(data.message);
                    }
                } catch (error)
                {
                    Alerta.TratamentoErroComLinha("agendamento_viagem.js", "success", error);
                }
            },
            error: function (err)
            {
                try
                {
                    console.log("Erro:  " + err.responseText);
                    alert("something went wrong");
                } catch (error)
                {
                    Alerta.TratamentoErroComLinha("agendamento_viagem.js", "error", error);
                }
            }
        });
    } catch (error)
    {
        Alerta.TratamentoErroComLinha("agendamento_viagem.js", "excluirAgendamento", error);
    }
}

