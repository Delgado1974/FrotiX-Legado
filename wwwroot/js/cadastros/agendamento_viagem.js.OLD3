// Uma Ãºnica instÃ¢ncia que cuida de todos os [data-ejtip]
let ejTooltip = new ej.popups.Tooltip({
    target: '[data-ejtip]',
    position: 'TopCenter',
    cssClass: 'ftx-ej-tooltip',
    opensOn: 'Hover',
    showTipPointer: false,
    beforeRender(args)
    {
        try
        {
            this.content = args.target.getAttribute('data-ejtip') || '';
        } catch (error)
        {
            Alerta.TratamentoErroComLinha("agendamento_viagem.js", "anonymous", error);
        }
    },
    afterOpen(args)
    {
        try
        {
            clearTimeout(this._autoCloseTimer);
            this._currentTarget = args.target;
            this._autoCloseTimer = setTimeout(() =>
            {
                try
                {
                    // sÃ³ fecha se ainda estiver aberta para o mesmo alvo
                    if (this._currentTarget === args.target) this.close();
                } catch (error)
                {
                    Alerta.TratamentoErroComLinha("agendamento_viagem.js", "setTimeout_callback", error);
                }
            }, 1000);
        } catch (error)
        {
            Alerta.TratamentoErroComLinha("agendamento_viagem.js", "anonymous", error);
        }
    },
    beforeClose()
    {
        try
        {
            clearTimeout(this._autoCloseTimer);
            this._currentTarget = null;
        } catch (error)
        {
            Alerta.TratamentoErroComLinha("agendamento_viagem.js", "anonymous", error);
        }
    }
});

