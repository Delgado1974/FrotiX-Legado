                // Tooltips Syncfusion
                document.querySelectorAll(".e-tooltip-wrap").forEach(t =>
                {
                    try
                    {
                        return t.remove();
                    } catch (error)
                    {
                        Alerta.TratamentoErroComLinha("agendamento_viagem.js", "forEach_callback", error);
                    }
                });
                document.querySelectorAll(".e-control.e-tooltip").forEach(el =>
                {
                    try
