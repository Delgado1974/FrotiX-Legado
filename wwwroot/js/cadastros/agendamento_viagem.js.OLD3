/**
 * Callback de fonte de eventos do FullCalendar (SEM '+1 dia' e SEM ajustes manuais).
 * Espera que o backend jÃ¡ devolva start/end coerentes (UTC com 'Z' ou local sem 'Z').
 */
async function calendarEvents(fetchInfo, successCallback, failureCallback)
{
    try
    {
        // Exemplo de chamada; ajuste a URL conforme sua API real:
        const resp = await fetch(`/api/Agenda/Eventos?start=${fetchInfo.startStr}&end=${fetchInfo.endStr}`);
        const payload = await resp.json();

        // Mapeia diretamente, sem addDaysLocal nem moment.add(1,'day')
        const events = (payload?.data || payload || []).map(item =>
        {
            try
            {
                return {
                    id: item.id,
                    title: item.title || item.descricao || "",
                    description: item.descricao || "",
                    start: item.start,
                    // sem '+1 dia'
                    end: item.end,
                    // sem '+1 dia'
                    backgroundColor: item.backgroundColor || undefined,
                    textColor: item.textColor || undefined,
                    allDay: item.allDay === true
                };
            } catch (error)
            {
                Alerta.TratamentoErroComLinha("agendamento_viagem.js", "map_callback", error);
            }
        });
        successCallback(events);
    } catch (error)
    {
        Alerta.TratamentoErroComLinha("agendamento_viagem.js", "calendarEvents", error);
        failureCallback(error);
    }
}

/**
 * Envia atualizaÃ§Ã£o de agendamento para o backend.
 * Trate tanto { success: true } quanto { data: true } como sucesso.
 * Exibe toastr e retorna true/false conforme o resultado.
 */
const aplicarAtualizacao = async objViagem =>
{
    try
    {
        const response = await fetch("/api/Agenda/Agendamento", {
            method: "POST",
            headers: {
                "Content-Type": "application/json"
            },
            body: JSON.stringify(objViagem)
        });
        const data = await response.json();
        if (data?.success || data?.data)
        {
            if (window.toastr) toastr.success(data.message || "Agendamento Atualizado");
            return true;
        } else
        {
            if (window.toastr) toastr.error(data?.message || "Falha ao atualizar agendamento");
            return false;
        }
    } catch (error)
    {
        Alerta.TratamentoErroComLinha("agendamento_viagem.js", "aplicarAtualizacao", error);
    }
};

