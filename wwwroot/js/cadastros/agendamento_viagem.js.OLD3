// ðŸ“Œ DescriÃ§Ã£o: FunÃ§Ã£o de espera para adicionar atraso entre as requisiÃ§Ãµes;
// ðŸ“¤ Chama: [#btnCancela, #btnApaga]
// ðŸ“¥ Chamado por: [Nenhuma funÃ§Ã£o detectada]
// âœ… Status: Ativa
// ===================================================================

function delay(ms)
{
    try
    {
        return new Promise(resolve =>
        {
            try
            {
                return setTimeout(resolve, ms);
            } catch (error)
            {
                Alerta.TratamentoErroComLinha("agendamento_viagem.js", "anonymous", error);
            }
        });
    } catch (error)
    {
        Alerta.TratamentoErroComLinha("agendamento_viagem.js", "delay", error);
    }
}

