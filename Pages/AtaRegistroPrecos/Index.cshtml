@page
@model FrotiX.Models.AtaRegistroPrecos

@{
	ViewData["Title"] = "Atas de Registro de Preços";
	ViewData["PageName"] = "atas_index";
	ViewData["Heading"] = "<i class='fa-duotone fa-folders'></i> Cadastros: <span class='fw-300'>Atas de Registro de Preços</span>";
	ViewData["Category1"] = "Cadastros";
	ViewData["PageIcon"] = "fa-regular fa-folders";
}

@section HeadBlock {
}

<style>
	.fundo-cinza {
		background-color: #2F4F4F;
		color: aliceblue;
	}

	.label {
		color: white;
	}
</style>

<div class="row">
	<div class="col-xl-12">
		<div id="panel-1" class="panel">
			<h2 class="titulo-paginas mb-0">
				<i class="fa-duotone fa-folders"></i>
				Atas de Registro de Preços
			</h2>
			<!-- Linha divisória -->
			<div class="col-12" style="border-bottom:1px solid #325d88"></div>

			<div class="panel-container show">
				<div class="panel-content float-right">
					<a href="/AtaRegistroPrecos/Upsert" class="btn btn-info" data-ejtip="Adicionar nova Ata de Registro de Preços">
						<i class="fa-regular fa-folders"></i> &nbsp; Adicionar Ata
					</a>
				</div>

				<div class="panel-content">
					<div class="box-body">
						<br /><br />
						<table id="tblAta" class="table table-bordered table-striped" width="100%">
							<thead>
								<tr>
									<th>Ata</th>
									<th>Processo</th>
									<th>Objeto</th>
									<th>Empresa</th>
									<th>Vigência</th>
									<th>Valor</th>
									<th>Status</th>
									<th>Ação</th>
								</tr>
							</thead>
							<tbody>
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

@section ScriptsBlock {

	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.8/dist/css/bootstrap.min.css" crossorigin="anonymous" />
	<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.8/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>

	<script src="~/js/cadastros/ata.js" asp-append-version="true"></script>
}
